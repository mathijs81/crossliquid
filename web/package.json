{
  "name": "svelte-scaffold-eth",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "biome format --write",
    "generate": "wagmi generate && NODE_ENV=production wagmi generate",
    "lint": "biome lint --write",
    "fix": "biome lint --write && biome format --write",
    "test:unit": "vitest",
    "test:unit:run": "vitest --run",
    "test": "pnpm test:unit:run && pnpm test:e2e",
    "test:e2e": "playwright test"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.13",
    "@playwright/test": "^1.58.1",
    "@sveltejs/adapter-auto": "^7.0.0",
    "@sveltejs/adapter-static": "^3.0.10",
    "@sveltejs/kit": "^2.50.1",
    "@sveltejs/vite-plugin-svelte": "^6.2.4",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/svelte-query-devtools": "^6.0.4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/svelte": "^5.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^25.2.0",
    "@vitest/browser-playwright": "^4.0.18",
    "@vitest/coverage-v8": "4.0.18",
    "@vitest/ui": "4.0.18",
    "@wagmi/cli": "^2.9.0",
    "jsdom": "^27.4.0",
    "phosphor-svelte": "^3.1.0",
    "playwright": "^1.58.1",
    "svelte": "^5.49.1",
    "svelte-check": "^4.3.6",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "@tanstack/svelte-query": "^6.0.18",
    "@wagmi/connectors": "^7.1.5",
    "@wagmi/core": "^3.3.1",
    "abitype": "^1.2.3",
    "daisyui": "^5.5.16",
    "runed": "^0.37.1",
    "svelte-sonner": "^1.0.7",
    "viem": "^2.45.1"
  }
}
