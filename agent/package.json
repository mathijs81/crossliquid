{
  "name": "@crossliquid/agent",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "format": "biome format --write",
    "lint": "biome lint --write",
    "fix": "biome lint --write && biome format --write",
    "test": "vitest",
    "test:run": "vitest --run",
    "check": "tsc --noEmit"
  },
  "dependencies": {
    "fastify": "^5.1.0",
    "pino": "^9.7.0",
    "pino-pretty": "^13.0.0",
    "viem": "^2.45.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.13",
    "@types/node": "^25.2.0",
    "tsx": "^4.19.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
