{
  "name": "@crossliquid/agent",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "cli": "tsx --env-file=.env src/cli.ts",
    "format": "biome format --write",
    "lint": "biome lint --write",
    "fix": "biome lint --write && biome format --write",
    "test": "vitest",
    "test:run": "vitest --run",
    "check": "tsc --noEmit",
    "generate": "bash ../foundry/copyAbis.sh",
    "gen-docker-image": "docker build -f prod/Dockerfile -t crossliquid-agent ."
  },
  "dependencies": {
    "@lifi/sdk": "^3.15.5",
    "@uniswap/universal-router-sdk": "^4.30.0",
    "@uniswap/v4-sdk": "^1.27.0",
    "better-sqlite3": "^12.6.2",
    "es-toolkit": "^1.44.0",
    "fastify": "^5.7.4",
    "pino": "^10.3.0",
    "pino-pretty": "^13.1.3",
    "viem": "^2.45.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.14",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^25.2.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
